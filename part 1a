//Norah Nwaneri
#include <iostream>
#include <vector>
#include <iomanip>
using namespace std;

vector<vector<int> >& allCombinations(int N, int prev, vector<int> v, vector<vector<int> > &vPrime){
	if (N < 0)
		return vPrime;
	if (N == 0)
	{
		vPrime.push_back(v);
		return vPrime;
	}
	v.push_back(0);
	for (int i = prev; i > 0; i--)
	{
		v.pop_back();
		v.push_back(i);
		allCombinations(N - i, i, v, vPrime);
	}
}

void printVect(vector<int> &myVector)
{
	int i = 0, size = myVector.size();
	for (i = 0;i < size;i++)
		cout << myVector[i];
	cout << "\n";
}
void printVects(vector<vector<int> > &myVector)
{
	int i, size = myVector.size();
	for (i = 0; i < size; i++)
		printVect(myVector[i]);
}

int main(int argc, char const *argv[])
{
	vector<vector<int> > u;
	vector<int> v;
	allCombinations(9, 9 - 1, v, u);
	printVects(u);
	return 0;
}
